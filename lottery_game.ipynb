    "import random\n",
    "\n",
    "number=[]\n",
    "count=0 \n",
    "\n",
    "while(count<7):\n",
    "    x=random.randint(1,45)\n",
    "    if(x not in number or count==0): #첫번째와 중복되는 경우 제외\n",
    "        number.append(x)\n",
    "        count+=1\n",
    "\n",
    "print(number[0:6],number[6:])"
   ]
